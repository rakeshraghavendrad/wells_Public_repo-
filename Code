SELECT
  Custnum,
  MAX(CASE WHEN rn = 1 THEN Ip_addar END) AS first_login_ip,
  MAX(CASE WHEN rn = 2 THEN Ip_addar END) AS second_login_ip,
  DATEDIFF(MINUTE, MAX(CASE WHEN rn = 1 THEN Evnt_start_dttm END), MAX(CASE WHEN rn = 2 THEN Evnt_start_dttm END)) AS time_difference_minutes
FROM (
  SELECT
    Custnum,
    Ip_addar,
    Evnt_start_dttm,
    ROW_NUMBER() OVER (PARTITION BY Custnum ORDER BY Evnt_start_dttm) AS rn
  FROM multloginevnt
) AS ranked
WHERE rn IN (1, 2)
GROUP BY Custnum;
