SELECT
  schema1.customer_id,
  schema1.customer_name AS schema1_customer_name,
  schema2.customer_name AS schema2_customer_name
FROM
  schema1.customer_table schema1
WHERE NOT EXISTS (
  SELECT 1
  FROM schema2.customer_table schema2
  WHERE schema1.customer_id = schema2.customer_id
    AND schema1.customer_name = schema2.customer_name
);
