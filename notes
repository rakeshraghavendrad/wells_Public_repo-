from matplotlib_venn import venn2
import matplotlib.pyplot as plt

# Assuming c1_values and c2_values are the sets of unique values in c1 and c2 respectively
# You can get these sets from your DataFrame using distinct() or other operations

# Convert sets to lists for matplotlib_venn
c1_list = c1_values.collect()
c2_list = c2_values.collect()

# Create the Venn diagram
venn2(subsets=(len(c1_list) - len(c2_list.intersection(c1_list)),
               len(c2_list) - len(c2_list.intersection(c1_list)),
               len(c2_list.intersection(c1_list))),
      set_labels=('c1', 'c2'))
plt.title('Venn Diagram of c1 and c2')
plt.show()
