def extract_key_value_pairs(image_array, regions):
    key_value_pairs = {}
    for (x, y) in regions:
        region = image_array[x:x+20, y:y+100]
        text = ''.join(chr(int(c)) for c in region.flatten() if 32 <= c <= 126)
        if ':' in text:
            key, value = text.split(':', 1)
            key_value_pairs[key.strip()] = value.strip()
    return key_value_pairs
