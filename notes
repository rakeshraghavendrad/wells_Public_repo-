import matplotlib.pyplot as plt

# Assuming df is your PySpark DataFrame
# Select the two columns c1 and c2
selected_cols = df.select('c1', 'c2').toPandas()

# Plot scatter plot
plt.scatter(selected_cols['c1'], selected_cols['c2'])
plt.title('Scatter Plot between c1 and c2')
plt.xlabel('c1 values')
plt.ylabel('c2 values')
plt.show()
