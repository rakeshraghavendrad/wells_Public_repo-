# Function to print executor logs
def print_executor_logs():
    # Fetch executor logs
    executor_logs = spark.sparkContext.parallelize([""]).map(lambda x: spark.sparkContext.getConf().get("spark.executor.logs.rolling.strategy")).collect()

    # Print executor logs
    logger.info("Executor Logs:")
    for log in executor_logs:
        logger.info(log)

# Print executor logs
print_executor_logs()
