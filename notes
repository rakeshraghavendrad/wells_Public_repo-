import matplotlib.pyplot as plt

# Assuming you have the result DataFrame from the previous step
x = result.select('sent_in').collect()
y = result.select('num_customers').collect()

x_labels = [row[0] for row in x]
y_values = [row[0] for row in y]

plt.bar(x_labels, y_values)
plt.xlabel('sent_in')
plt.ylabel('Number of Customers')
plt.title('Number of Customers Grouped by sent_in')
plt.show()
