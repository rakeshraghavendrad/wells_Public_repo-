import pandas as pd

# Sample DataFrame
data = {
    'fullname.30': ['John Paul Doe', 'Jane Marie Smith', 'Jim Bob Brown'],
    'fullname.90': ['John Paul Doe', 'Jane Smith', 'Jim Brown']
}
df = pd.DataFrame(data)

# Convert entire DataFrame to lowercase
df_lower = df.applymap(lambda x: x.lower() if isinstance(x, str) else x)

print(df_lower)
