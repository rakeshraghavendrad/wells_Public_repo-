import zipfile

def check_jar_contents(jar_path):
    print(f"Contents of JAR file '{jar_path}':")
    try:
        with zipfile.ZipFile(jar_path, 'r') as zip_ref:
            contents = zip_ref.namelist()
            if "com/teradata/jdbc/TeraDriver.class" in contents:
                print("The required class 'com.teradata.jdbc.TeraDriver' is present.")
            else:
                print("Warning: The required class 'com.teradata.jdbc.TeraDriver' is not present.")
    except zipfile.BadZipFile:
        print("Error: Invalid JAR file.")

# Replace '/path/to/terajdbc4.jar' with the actual path to your JAR file
check_jar_contents('/path/to/terajdbc4.jar')
